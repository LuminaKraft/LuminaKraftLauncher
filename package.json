{
  "name": "luminakraft-launcher",
  "private": true,
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev --no-watch",
    "tauri:dev-stable": "node scripts/kill-port.js 1420 && npm run tauri dev",
    "tauri:dev-watch": "tauri dev",
    "tauri:build": "tauri build",
    "kill-port": "node scripts/kill-port.js",
    "release": "node scripts/release.js",
    "release:push": "node scripts/release.js --push",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:patch-pre": "node scripts/release.js patch --prerelease",
    "release:minor-pre": "node scripts/release.js minor --prerelease",
    "release:major-pre": "node scripts/release.js major --prerelease",
    "release:patch-push": "node scripts/release.js patch --push",
    "release:minor-push": "node scripts/release.js minor --push",
    "release:major-push": "node scripts/release.js major --push",
    "release:patch-pre-push": "node scripts/release.js patch --prerelease --push",
    "release:minor-pre-push": "node scripts/release.js minor --prerelease --push",
    "release:major-pre-push": "node scripts/release.js major --prerelease --push",
    "clean": "rm -rf node_modules/.vite && rm -rf dist && rm -rf src-tauri/target"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "^2.2.1",
    "axios": "^1.6.2",
    "dotenv": "^16.5.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.2",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "wait-on": "^8.0.3"
  },
  "isPrerelease": true
}
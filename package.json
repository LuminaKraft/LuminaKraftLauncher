{
  "name": "luminakraft-launcher",
  "private": true,
  "version": "1.0.0-rc.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev --no-watch",
    "tauri:dev-stable": "node kill-port.js && tauri dev --no-watch",
    "tauri:dev-watch": "tauri dev",
    "tauri:build": "tauri build",
    "kill-port": "node kill-port.js",
    "release": "node release.js",
    "release:patch": "node release.js patch --push",
    "release:minor": "node release.js minor --push",
    "release:major": "node release.js major --push",
    "release:patch-pre": "node release.js patch --prerelease --push",
    "release:minor-pre": "node release.js minor --prerelease --push",
    "release:major-pre": "node release.js major --prerelease --push",
    "release:pre": "node scripts/release-pre.js",
    "release:version": "node scripts/release-version.js",
    "test:release": "node test-release.js"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "^2.2.1",
    "axios": "^1.6.2",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.2",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "wait-on": "^8.0.3"
  },
  "isPrerelease": true
}
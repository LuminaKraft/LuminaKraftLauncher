{
  "name": "luminakraft-launcher",
  "private": true,
  "version": "0.0.9-alpha.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "node scripts/check-linux-deps.js && tauri dev --no-watch",
    "tauri:dev-stable": "node scripts/kill-port.js 1420 && npm run tauri:dev",
    "tauri:dev-watch": "node scripts/check-linux-deps.js && tauri dev",
    "tauri:build": "node scripts/check-linux-deps.js && tauri build",
    "kill-port": "node scripts/kill-port.js",
    "release": "node scripts/release.js",
    "release:push": "node scripts/release.js --push",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:patch-push": "node scripts/release.js patch --push",
    "release:minor-push": "node scripts/release.js minor --push",
    "release:major-push": "node scripts/release.js major --push",
    "release:alpha": "node scripts/release.js",
    "release:alpha-push": "node scripts/release.js --push",
    "generate-prerelease": "node scripts/generate-prerelease-manifest.cjs",
    "clean": "rm -rf node_modules/.vite && rm -rf dist && rm -rf src-tauri/target"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.6.0",
    "@tauri-apps/plugin-dialog": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "^2.3.0",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "axios": "^1.6.2",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tauri-apps/cli": "^2",
    "@types/node": "^24.0.3",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.6.2",
    "vite": "^6.0.3"
  },
  "isPrerelease": true
}